#!/bin/bash

source $(dirname $(dirname $(realpath ${BASH_SOURCE[0]})))/conf/env || exit 1
export CRON_APP_DIR=${CRON_APPS}/admindashboard-cli
source ${CRON_CONF}/init || exit 1

RUN_ID=$(${CRON_BIN}/healthcheck-start "eb266fe6-727d-4ce5-9994-50ffe987d54e" $RUN_ID)
php ./bin/admindashboard.php createCourses
php ./bin/admindashboard.php createEnrollment
${CRON_BIN}/healthcheck-finish "eb266fe6-727d-4ce5-9994-50ffe987d54e" $RUN_ID
